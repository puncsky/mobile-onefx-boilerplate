{
  "expo": {
    "version": "0.1.0",
    "name": "mobile-onefx-boilerplate",
    "slug": "mobile-onefx-boilerplate",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.js.onefx.ios"
    },
    "android": {
      "versionCode": 10,
      "package": "org.js.onefx.android",
      "googleServicesFile": "./google-services.json"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "TODO: sentry organization",
            "project": "TODO: sentry project",
            "authToken": "TODO: sentry authToken"
          }
        }
      ]
    },
    "notification": {
      "icon": "./src/assets/images/icon96.png"
    }
  }
}
