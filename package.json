{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "_note": "npm run lint && npm run tsc-test && ",
    "test": "npm run lint && jest",
    "jest": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "pretty-quick --staged && tslint --fix --project tsconfig.json -t stylish",
    "qualitycheck": "npm-run-all typecheck tslint",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/{*.ts,**/*.ts,*.tsx,**/*.tsx}\"\n"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.10",
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^9.0.0",
    "@react-native-community/async-storage": "^1.6.3",
    "@types/expo": "31.0.8",
    "@types/react-navigation": "2.13.10",
    "@types/react-redux": "7.0.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "expo": "33.0.0",
    "expo-asset": "^5.0.1",
    "expo-constants": "~5.0.0",
    "expo-font": "~5.0.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "react": "16.8.3",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-navigation": "2.18.3",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-test-renderer": "^16.0.3",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-expo": "^5.1.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.7.0",
    "ts-jest": "^23.10.5",
    "tslint": "5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "6.0.0",
    "tslint-require-connnect-typing": "1.0.1",
    "typescript": "3.2.4"
  },
  "private": true
}
